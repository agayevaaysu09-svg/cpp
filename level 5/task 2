#include <iostream>
using namespace std;


double orta(int a, int b, int c) {
    return (a + b + c) / 3.0;
}

int main() {
    int a, b, c;

    cout << "3 eded daxil edin: ";
    cin >> a ;
    cin >> b ;
    cin >> c ;

    double netice = orta(a, b, c);

    cout << "Ededi orta: " << netice;

    return 0;
}